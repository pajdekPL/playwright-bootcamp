{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "baseUrl": ".",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "paths": {
      "@data/*": ["src/data/*"],
      "@pages/*": ["src/pages/*"],
      "@utils/*": ["src/utils/*"],
      "@fixtures/*": ["src/fixtures/*"]
    }
  }
}
